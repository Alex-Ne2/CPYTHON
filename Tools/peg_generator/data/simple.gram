start: a=stmt+ ENDMARKER { _PyPegen_make_module(p, a) }
stmt:
    | a=expr NEWLINE { _Py_Expr(a, EXTRA) }
    | a=if_stmt NEWLINE { a }
if_stmt:
    | 'if' a=NAME ':' b=expr { _Py_If(a, b, NULL, EXTRA) }
expr:
    | a=term '+' b=expr { _Py_BinOp(a, Add, b, EXTRA) }
    | term
term: 
    | a=factor '*' b=term { _Py_BinOp(a, Mult, b, EXTRA) }
    | factor
factor:
    | '(' a=expr ')' { a }
    | '[' a=','.expr+ ']' { _Py_List(a, Load, EXTRA) }
    | NUMBER
    | NAME
